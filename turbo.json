{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"$TURBO_DEFAULT$",
				".env*"
			],
			"outputs": [
				"dist/**"
			],
			"env": [
				"CLERK_JWT_ISSUER_DOMAIN",
				"CLERK_SECRET_KEY",
				"GOOGLE_GEMINI_API_KEY"
			]
		},
		"lint": {
			"dependsOn": [
				"^lint"
			]
		},
		"check-types": {
			"dependsOn": [
				"^check-types"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dev:setup": {
			"cache": false,
			"persistent": true
		}
	}
}